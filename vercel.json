{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "status": 200
        }
    ],
    "ignoreCommand": "tsc --noEmit",
    "cleanUrls": true,
    "trailingSlash": false
}
